{
  "browserSupport": [
    "chrome 90",
    "firefox 88", 
    "safari 14",
    "edge 90"
  ],
  "warningLevel": "warning",
  "autoCheck": true,
  "cacheDuration": 3600,
  "excludePatterns": [
    "node_modules/**",
    "dist/**",
    "build/**",
    "*.min.js",
    "*.min.css"
  ],
  "customFeatures": [],
  "teamSettings": {
    "notifyOnNewFeatures": true,
    "requireApprovalForLimited": true,
    "autoUpdateBaseline": true
  }
}
